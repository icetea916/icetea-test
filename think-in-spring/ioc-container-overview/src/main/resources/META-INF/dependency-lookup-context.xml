<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="user" class="life.icetea.test.thinkinspring.ioc.overview.life.icetea.test.beancopire.domain.UserEntity">
        <property name="id" value="1"/>
        <property name="username" value="icetea"/>
    </bean>

    <bean id="objectFactory" class="org.springframework.beans.factory.config.ObjectFactoryCreatingFactoryBean">
        <property name="targetBeanName" value="user"/>
    </bean>

    <bean id="superUser" class="life.icetea.test.thinkinspring.ioc.overview.life.icetea.test.beancopire.domain.SuperUser"
          parent="user" primary="true">
        <property name="address" value="北京"/>
    </bean>

</beans>